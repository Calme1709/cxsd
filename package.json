{
  "name": "cxsd",
  "version": "0.2.0",
  "description": "Streaming XSD parser and XML parser generator with TypeScript output",
  "bin": {
    "cxsd": "bin/cxsd"
  },
  "scripts": {
    "tsc": "tsc",
    "old-postinstall-cget": "mkdir -p cget && rm -rf cget && git clone https://github.com/charto/cget.git && cd cget && git checkout 5c16eef4f59e0fe6b65ce5f9d176739be5ba58c0 && npm install && cd .. && npm link ./cget",
    "old-postinstall-cxml": "mkdir -p cxml && rm -rf cxml && git clone https://github.com/charto/cxml.git && cd cxml && git checkout 1f55f2fbe9e5e70ee4b98352078500f4a9bed6d6 && npm install && cd .. && npm link ./cxml",
    "build": "tsc -p tsconfig.json",
    "test": "npm-run-all test:*",
    "test:build": "npm-run-all test:build:*",
    "test:build:clean": "cd test && mkdir -p xmlns && rm -rf xmlns && cd ..",
    "test:build:compile": "cd test && node ../bin/cxsd https://raw.githubusercontent.com/ariutta/GPML/e427899e179919e220280f5cf93948c8d9ae7dc8/GPML2013a.xsd && cd ..",
    "test:build:rename": "cd test && mv ./xmlns/www.biopax.org/release/biopax-level3.owl_.d.ts ./xmlns/www.biopax.org/release/biopax-level3.owl.d.ts && mv ./xmlns/www.biopax.org/release/biopax-level3.owl_.js ./xmlns/www.biopax.org/release/biopax-level3.owl.js && cd ..",
    "test:run": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "((/src/.*\\.test\\.(ts|tsx|js))|(/test/spec/.*\\.(ts|tsx|js)))$"
  },
  "author": "Juha JÃ¤rvi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charto/cxsd.git"
  },
  "bugs": {
    "url": "https://github.com/charto/cxsd/issues"
  },
  "homepage": "https://github.com/charto/cxsd#readme",
  "keywords": [
    "xsd",
    "dts",
    "xml",
    "schema",
    "parser",
    "typescript"
  ],
  "dependencies": {
    "@types/bluebird": "^3.5.5",
    "@types/commander": "^2.9.1",
    "@types/node": "^7.0.31",
    "bluebird": "^3.5.0",
    "@wikipathways/cget": "^0.1.0-1",
    "commander": "^2.9.0",
    "@wikipathways/cxml": "^0.2.1-1",
    "node-expat": "^2.3.15"
  },
  "devDependencies": {
    "@types/jest": "^19.2.4",
    "jest": "^20.0.4",
    "npm-run-all": "^4.0.2",
    "ts-jest": "^20.0.6",
    "tslint": "^4.3.1",
    "typescript": "^2.3.4"
  }
}
